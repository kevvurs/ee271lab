module storeDisplay(DU0, DU1, DU2, DU3, DU4, DU5, U, P, C);
	input logic U, P, C;
	output logic [6:0] DU0, DU1, DU2, DU3, DU4, DU5;
	logic [2:0] code;
	
	code[2] = U;
	code[1] = P;
	code[0] = C;
	always_comb
		case(code)
			3'b000:	DU0 = 7'b0000000;
						DU1 = 7'b0000000;
						DU2 = 7'b0000000;
						DU3 = 7'b0000000;
						DU4 = 7'b0000000;
						DU5 = 7'b0000000;
			3'b001:	DU0 = 7'b0000000;
						DU1 = 7'b0000000;
						DU2 = 7'b0000000;
						DU3 = 7'b0000000;
						DU4 = 7'b0000000;
						DU5 = 7'b0000000;
			3'b011:	DU0 = 7'b0000000;
						DU1 = 7'b0000000;
						DU2 = 7'b0000000;
						DU3 = 7'b0000000;
						DU4 = 7'b0000000;
						DU5 = 7'b0000000;
			3'b100:	DU0 = 7'b0000000;
						DU1 = 7'b0000000;
						DU2 = 7'b0000000;
						DU3 = 7'b0000000;
						DU4 = 7'b0000000;
						DU5 = 7'b0000000;
			3'b101:	DU0 = 7'b0000000;
						DU1 = 7'b0000000;
						DU2 = 7'b0000000;
						DU3 = 7'b0000000;
						DU4 = 7'b0000000;
						DU5 = 7'b0000000;
			3'b110:	DU0 = 7'b0000000;
						DU1 = 7'b0000000;
						DU2 = 7'b0000000;
						DU3 = 7'b0000000;
						DU4 = 7'b0000000;
						DU5 = 7'b0000000;
			default: DU0 = 7'bX;
						DU1 = 7'bX;
						DU2 = 7'bX;
						DU3 = 7'bX;
						DU4 = 7'bX;
						DU5 = 7'bX;
endmodule
